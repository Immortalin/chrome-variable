{
    "manifest_version": 2,
    "name": "chrome-extension",
    "description": "a chrome extension for simplifying processes in forms.",
    "version": "1.0",
    "browser_action":{
        "default_icon":"icon.png",
        "default_popup":"popup.html"
    },
    "content_scripts": [
        {
          "matches": ["*://*/*"],
          "run_at": "document_end",
          "js": ["contentScript.js"]
        }
      ],
    "permissions":["storage"]
  }