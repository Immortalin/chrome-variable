{
    "manifest_version": 2,
    "name": "chrome-extension",
    "description": "a chrome extension for simplifying processes in forms.",
    "version": "1.0",
    "background":{
        "scripts":["background.js"],
        "persistant":false
    },
    "browser_action":{},
    "content_scripts": [{
        "matches": ["*://*/*"],
        "all_frames": true,
        "js":["popup.js"],
        "run_at":"document_end"
        }
     ]
  }